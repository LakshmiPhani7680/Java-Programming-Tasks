import java.util.Scanner;
public class CubeGivenInteger
{
public static void main(String [] Laks)
{
    Scanner scan=new Scanner (System.in);
    int num=scan.nextInt();
    for(int i=1;i<=num;i++)
    {
        int cube =(i*i*i);
 System.out.println(" Number is : " + i + " and cube of " + i + " is : " + cube);
    } 
}
}
